{
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true
    },
    "globals": {
        "$": true,
        "jQuery": true,
        "define": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}